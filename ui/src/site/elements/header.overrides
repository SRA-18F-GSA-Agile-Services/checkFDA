/*******************************
         Site Overrides
*******************************/

.ui.header {
	min-height: 138px;
	background-color: @checkFDABlue;
	margin: 0;
	padding: 40px;
	& > a.logo {
		display: block;
		width: 287px;
		height: 55px;
		background: url("@{checkFDAimagePath}/logo.png") 0 0 no-repeat;
		text-indent: 100%;
		white-space: nowrap;
		overflow: hidden;
	}
}
