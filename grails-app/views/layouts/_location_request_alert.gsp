<div class="ui modal">
	<i class="close icon"></i>
	<div class="header">
		<g:message code="location.request.alert.title" />
	</div>
	<div class="content">
		<div class="description">
			<div class="header">
				<g:message code="location.request.alert.message" />
			</div>
		</div>
	</div>
	<div class="actions">
		<div class="ui negative button">
			No
		</div>
		<div class="ui positive button">
			Yes
		</div>
	</div>
</div>